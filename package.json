        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-ajax": "^0.1.13",
        "karma-jasmine-html-reporter": "^1.7.0",
        "karma-mocha-reporter": "^2.2.5",
        "less-plugin-npm-import": "^2.1.0",
        "lint-staged": "^8.2.1",
        "markdown-it": "^12.3.2",
        "ng-morph": "^2.0.0",
        "ng-packagr": "^9.1.5",
        "postcss": "^8.4.6",
        "protractor": "~5.4.4",
        "raw-loader": "^4.0.2",
        "rollup": "~2.67.2",
        "rollup-plugin-typescript2": "~0.31.2",
        "standard-version": "^8.0.2",
        "svgo": "~2.8.0",
        "ts-loader": "^5.4.5",
        "ts-node": "~7.0.1",
        "typescript": "~3.8.3"
    },
    "stylelint": {
        "extends": [
            "@tinkoff/stylelint-config/less",
            "@tinkoff/stylelint-config/angular"
        ],
        "ignoreFiles": [
            "**/demo/**",
            "**/dist/**",
            "**/coverage/**",
            "**/node_modules/**"
        ]
    },
    "lint-staged": {
        "*.{js,ts,html,md,less,json}": [
            "npm run lint -- --fix",
            "prettier --write",
            "git add"
        ],
        "*.less": [
            "stylelint --fix",
            "git add"
        ]
    },
    "standard-version": {
        "scripts": {
            "postbump": "node scripts/sync-versions.js && node projects/core/scripts/version.js && git add **/package.json && git add projects/core/constants/version.ts"
        }
    },
    "engines": {
        "node": ">= 10",
        "npm": ">= 3 < 7",
        "yarn": "Please use npm instead of yarn to install dependencies"
    }
}
