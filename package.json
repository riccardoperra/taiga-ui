        "core-js": "^2.6.9",
        "dompurify": "2.2.3",
        "express": "^4.15.2",
        "highlight.js": "^11.3.1",
        "ngx-highlightjs": "^4.1.4",
        "ngx-markdown": "^11.1.3",
        "preboot": "^7.0.0",
