        "@types/resize-observer-browser": "0.1.5",
        "@types/svgo": "~2.6.0",
        "@types/markdown-it": "^12.2.3",
        "angular-http-server": "^1.10.0",
        "codelyzer": "^5.1.2",
        "concurrently": "^6.2.0",
        "cypress": "^7.5.0",
        "cypress-image-snapshot": "^4.0.1",
        "find-in-files": "0.5.0",
        "firebase-tools": "^9.21.0",
        "http-server": "^0.12.3",
        "husky": "7.0.2",
        "jasmine-core": "^3.4.0",
        "jasmine-spec-reporter": "^4.2.1",
