{
    "implicitDependencies": {
        "angular.json": "*",
        "package.json": {
            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.js": "*",
        "nx.json": "*",
        "karma.*.js": "*",
        "tsconfig.*.json": "*",
        "tsconfig.json": "*"
    },
    "npmScope": "taiga-ui",
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nx-cloud/runners/firebase",
            "options": {
                "cacheableOperations": ["build", "test", "lint"],
                "parallel": 1
            }
        }
    },
    "affected": {
        "defaultBase": "origin/main"
    },
    "workspaceLayout": {
        "libsDir": "projects",
        "appsDir": "projects"
    },
    "cli": {
        "analytics": "ce6ae05d-f6ae-4271-89e4-40e616f7090f"
    },
    "defaultProject": "demo"
}
